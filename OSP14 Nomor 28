uses crt;
var
a : integer;
function cimi(x,y :integer):integer;
begin
if (x + y = 0) then begin
   cimi := 0;
   end
else if (x > y) then begin
    cimi := y + cimi(x-1,y);
    end
    else
        begin
        cimi := x + cimi(x,y-1);
    end;
end;
begin
a := cimi(5,7);
write(a);
readkey;
end.


28. Berapakah nilai dari fungsi cimi(29,13)? 
Jawab : 
a:=cimi(29,13);
x:=29;
y:13;

1.	else if (29>13) ya
	cimi:= y + cimi (x-1,y)
	cimi:= 13  + cimi(28,13)

